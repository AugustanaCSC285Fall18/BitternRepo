      <Pane prefHeight="500.0" prefWidth="500.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="browseButton" layoutX="44.0" layoutY="298.0" mnemonicParsing="false" onAction="#handleBrowse" text="Browse" />
